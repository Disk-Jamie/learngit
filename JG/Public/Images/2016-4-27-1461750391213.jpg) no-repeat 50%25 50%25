<script type="text/javascript">
 //头部引入header.jsp
    if (window.location.host != "manage.neibu.koolearn.com") {
        var r=document.referrer,NDate = new Date();
        $("#head_top").load("/public/header_2.jsp?params.wd=w1200&r="+r+"&t=" +NDate.getFullYear()+""+NDate.getDate()+"&v="+NDate.getTime(), function(html){
            //去掉登录框
			$("#pop_login").remove();
			$("#head_top link").remove();
			//all-menus show
            $(".all-menus").find(".down-menu").removeClass("hide");
            //add advo
			var ad_timer = setInterval(function(){
				if($(".avlog").size()>0 && $.trim($(".avlog").html())==""){
					$(".avlog").html($("#hide_ad").find("a"));
				}else{ 
					ad_timer && clearInterval(ad_timer);
				}
			},1000);
            if($(".hotwords").size()>0)$(".hotwords").html($("#hotword").html());
            //FF 搜索框空白页
            $(".search_sub_btn").attr("onclick","doSubmit();return false;");
            if (!html) {
                window.location.href = "/public/header_2.jsp?params.wd=w1200&r="+r+"&t=" +new Date().getFullYear()+""+new Date().getDate() + "&next_page=" + window.location.href;
            }
            if (html == "error.user.kicked") {
                window.location.href = "/head/toLogin?errCode=error.login.kicked&next_page=" + window.location.href;
            }
        });
    }
</script>
